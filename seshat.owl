	    rdfs:comment "An indication of how centralized a political authority was"@en ;
	    rdfs:label "Degree of Centralization"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:DegreeOfCentralizationChoice
	    a owl:Class ;
	    rdfs:comment "The degree of centralisation of an organisation"@en ;
	    rdfs:label "Degree of Centralization Choice"@en ;
	    rdfs:subClassOf tcs:Enumerated ;
	    owl:oneOf (scm:quasipolity
	        scm:nominal
	        scm:loose
	        scm:confederated_state
	        scm:unitary_state
	        scm:unknown_centralisation
	        scm:no_centralisation
	    ) .
	
	scm:EntityReference
	    a owl:Class ;
