	scm:FeaturePresenceRecord
	    a owl:Class ;
	    rdfs:comment "An abstract class to represent a themed record about a particular historical system (postal system, legal system, ...) where the aspects of the system are most commonly encoded as epistemic state variables"@en ;
	    rdfs:label "Feature Presence Record"@en .
	
	scm:FoodStorage
	    a owl:Class ;
	    rdfs:comment "IV-4-1. Food Storage."@en ;
	    rdfs:label "Food Storage"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:FreeFormArea
	    a owl:Class ;
	    rdfs:comment "Freeform area (FA). This is an area enclosed within a boundary, or a dated set of boundaries. Basically, it is our method for treating an arbitrary shape on the map to which a historical expert wishes to tie certain values of coded variables. Examples include a province within an empire, something with a hole in it, or a \"southeast corner\" of a region."@en ;
	    rdfs:label "Free form area"@en ;
	    rdfs:subClassOf scm:UnitOfTerritory .
	
	scm:Frequency
	    a owl:Class ;
	    rdfs:comment "The frequency of some event"@en ;
	    rdfs:label "Frequency"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:FuzzilyTemporallyScoped
	    a owl:Class ;
	    rdfs:label "Temporally scoped with fuzzy range"@en ;
	    rdfs:subClassOf scm:FuzzyTemporalScoping, tcs:TemporallyScoped .
	
	scm:FuzzyLifespan
	    a owl:Class ;
	    rdfs:label "Fuzzy Lifespan Endpoint"@en ;
	    rdfs:subClassOf scm:FuzzyLifespanEndpointRangeRestriction, tcs:TimeRange .
	
	scm:FuzzyLifespanEndpoint
	    a owl:Class ;
	    rdfs:label "Fuzzy Lifespan Endpoint"@en ;
	    rdfs:subClassOf tbs:GYearRange, tcs:TimePoint.
	
	scm:FuzzyLifespanEndpointRangeRestriction
	    a owl:Restriction ;
	    rdfs:label "Sets the type of the start and end temporal range for this class"@en ;
	    owl:allValuesFrom scm:FuzzyLifespanEndpoint ;
	    owl:onProperty tcs:end, tcs:start .
	
	scm:FuzzyTemporalScoping
	    a owl:Restriction ;
	    rdfs:label "Sets the type of temporal restriction"@en ;
	    owl:allValuesFrom scm:FuzzyLifespan ;
	    owl:onProperty tcs:temporality .
	
