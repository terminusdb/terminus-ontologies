	    rdfs:label "Battle"@en ;
	    rdfs:subClassOf scm:ConflictEvent .
	
	scm:Building
	    a owl:Class ;
	    rdfs:comment "Used to describe impressive/costly buildings that are then associated with the Polities that built them and the NGAs in which they are placed. They have 3 dimensions and a narrative paragraph."@en ;
	    rdfs:label "Building"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:BureaucraticSystem
	    a owl:Class ;
	    rdfs:comment "Encodes the features of a historical bureaucratic system. "@en ;
	    rdfs:label "Bureaucratic System"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:BurialVariation
	    a owl:Class ;
	    rdfs:comment "Variation in burials and burial goods"@en ;
	    rdfs:label "Burial Variation"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:Ceremonials
	    a owl:Class ;
	    rdfs:comment "Prominent Community Ceremonials [AR-TL-6] (based on Murdock and Wilson 1972)"@en ;
	    rdfs:label "Ceremonials"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:ChangeInSize
	    a owl:Class ;
	    rdfs:comment "How much change has taken place in size and complexity"@en ;
	    rdfs:label "Change in size"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:ChangeSize
	    a owl:Class ;
	    rdfs:label "Change Size"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:ChangeSizeType
	    a owl:Class ;
	    rdfs:comment "How much change has taken place"@en ;
	    rdfs:label "Size of Change"@en ;
	    rdfs:subClassOf tcs:Enumerated ;
	    owl:oneOf (scm:big_decrease
	        scm:small_decrease
	        scm:stable
	        scm:small_increase
	        scm:big_increase
	    ) .
	
	scm:City
	    a owl:Class ;
	    rdfs:comment "Cities are represented by a single point on the map that doesn't change with time. Although it is possible to reflect their spatial expansion dynamically, we chose not to do so in the current implementation."@en ;
	    rdfs:label "City"@en ;
	    rdfs:subClassOf scm:Organisation .
	
	scm:CityReference
	    a owl:Class ;
	    rdfs:comment "A reference to a city"@en ;
	    rdfs:label "City Reference"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:CommunityIntegration
	    a owl:Class ;
	    rdfs:comment "Community integration, sum of scores [AR-TL-5] (based on Murdock and Wilson 1972)"@en ;
	    rdfs:label "Community Integration "@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:CommunitySizeChangeType
	    a owl:Class ;
	    rdfs:comment "How much change has taken place in size"@en ;
	    rdfs:label "Change in size"@en ;
	    rdfs:subClassOf tcs:Enumerated ;
	    owl:oneOf (scm:organization_collapses
	        scm:complexity_decreases
	        scm:stable_complexity
	        scm:complexity_increases
	        scm:big_complexity_increase
	    ) .
	
	scm:ConflictEvent
	    a owl:Class ;
	    rdfs:comment "An abstract class describing historical conflict events such as battles and wars."@en ;
	    rdfs:label "Conflict Event"@en ;
	    rdfs:subClassOf scm:Event .
	
	scm:CorporateExclusionary
	    a owl:Class ;
	    rdfs:label "Corporate Exclusionary"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:DegreeOfCentralization
