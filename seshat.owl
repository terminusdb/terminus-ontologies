	scm:EphemeralEntity
	    tcs:tag tcs:abstract ;
	    a owl:Class ;
	    rdfs:comment "The class of entities that has temporal boundaries on their existence - a birth date and/or a demise date"@en ;
	    rdfs:label "Ephemeral Entity"@en ;
	    rdfs:subClassOf scm:FuzzilyTemporallyScoped, tcs:Entity .
	
	scm:EpistemicFrequency
	    a owl:Class ;
	    rdfs:comment "The epistemic state / frequency of some variable"@en ;
	    rdfs:label "Epistemic Frequency"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:EpistemicFrequencyStateChoice
	    a owl:Class ;
	    rdfs:comment "The possible values of an epistemic state choice"@en ;
	    rdfs:label "Epistemic Frequency State Choice"@en ;
	    rdfs:subClassOf tcs:Enumerated ;
	    owl:oneOf (scm:unknown
	        scm:absent
	        scm:rare
	        scm:frequent
	    ) .
	
	scm:EpistemicState
