	    rdfs:label "Agricultural Diversity"@en ;
	    rdfs:subClassOf scm:FeaturePresenceRecord .
	
	scm:AlternativeFoodSources
	    a owl:Class ;
	    rdfs:comment "IV-4-3. Alternative Food Sources. [AR-SV-7] [AR-SV-9] [AR-SV-11] "@en ;
	    rdfs:label "Alternative Food Sources."@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:AuthorityEmphasis
	    a owl:Class ;
	    rdfs:comment "Where the emphasis of group authority lies"@en ;
	    rdfs:label "Authority Emphasis"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:AuthoritySharing
	    a owl:Class ;
	    rdfs:comment "How authority is shared between leaders and others"@en ;
	    rdfs:label "Authority Sharing"@en ;
	    rdfs:subClassOf tcs:QualifiedByConfidenceTag .
	
	scm:Battle
