  rdfs:range scm:EpistemicState .

scm:volume
  rdfs:label "Volume"@en ;
  rdfs:comment "Encodes the historical presence of a standard way of measuring volume. For example: pint, litre"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MeasurementSystem ;
  rdfs:range scm:EpistemicState .

scm:weight
  rdfs:label "Weight"@en ;
  rdfs:comment "Encodes the historical presence of a standard way of measuring weight. For example: pounds, kilograms"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MeasurementSystem ;
  rdfs:range scm:EpistemicState .

scm:articles
  rdfs:label "Articles"@en ;
  rdfs:comment "Articles with use-value used for exchange and trade, e.g. axes, cattle, grain."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:debt_and_credit
  rdfs:label "Debt and credit"@en ;
  rdfs:comment "Commercial and market debt and credit structures that take physical form, e.g. a contract on parchment (not just verbal agreements)"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:foreign_coins
  rdfs:label "Foreign Coins"@en ;
  rdfs:comment "Coins minted by some external polity are used for exchange"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:indigenous_coins
  rdfs:label "Indigenous Coins"@en ;
  rdfs:comment "Coins minted by local authority are in use for exchange"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:paper_currency
  rdfs:label "Paper Currency"@en ;
  rdfs:comment "Currency notes or other kind of fiat currency"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:precious_metals
  rdfs:label "Precious Metals"@en ;
  rdfs:comment "Bullion: non-coined silver, gold, platinum."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:stores_of_wealth
  rdfs:label "Stores of Wealth"@en ;
  rdfs:comment "Special places for storing wealth: (example: hoard, chest for storing valuables, treasury room). Note for the future: perhaps should separate these into individual variables."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .

scm:tokens
  rdfs:label "Tokens"@en ;
  rdfs:comment "Tokens used for exchange and trade, e.g. cowrie shells."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:MonetarySystem ;
  rdfs:range scm:EpistemicState .
 
 scm:bureaucratic_system
  rdfs:label "Bureaucratic System"@en ;
  rdfs:comment "The aspects of a bureaucratic system that were used by the political authority."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:BureaucraticSystem .

scm:capital_city
  rdfs:label "Capital City"@en ;
  rdfs:comment "The city where the ruler spends most of his or her time. If there was more than one capital, all are included. Note that the capital may be different from the largest city (see below).<p>'Capital' may be difficult to code for archaeologically known societies. If there is reasonable basis to believe that the largest known settlement was the seat of the ruler it is coded as capital. Archaeologists are able to recognize special architectural structures, such as a ceremonial centres and some kind of citadels or palaces. These features could be recognized with certainty after a careful study of the whole region and the settlement network. If such an inference cannot be made, this variable is coded as unknown (again, the largest settlement is coded elsewhere).</p>"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range tbs:String .

scm:degree_of_centralization
  rdfs:label "Degree of Centralization"@en ;
  rdfs:comment "How centralized was power in this political authority?"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:DegreeOfCentralization .

scm:hierarchical_complexity
  rdfs:label "Hierarchical Complexity"@en ;
  rdfs:comment "Encodes the number of levels in the most important hierarchies of a social system."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:HierarchicalComplexity .

scm:largest_settlement_population
  rdfs:label "Largest Settlement Population"@en ;
  rdfs:comment "Population count of the largest settlement within the entity. <p>Note that the largest settlement could be different from the capital. Where possible, the dynamics (that is, how population changed during the temporal period of the polity) are included in the notes. Note that we are also building a city database - this will be merged into that eventually.</p>"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range tbs:Integer .

 scm:legal_system
  rdfs:label "Legal System"@en ;
  rdfs:comment "The aspects of a legal system that were used by the political authority."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:LegalSystem .

scm:linguistic_family
  rdfs:label "Linguistic Family"@en ;
  rdfs:comment "The Linguistic family or families that the main languages belonged to. https://en.wikipedia.org/wiki/List_of_language_families"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range tbs:String .

scm:longest_communication_distance
  rdfs:label "Longest Communication Distance"@en ;
  rdfs:comment "Distance in kilometers between the capital and the furthest provincial capital. The figure for the most direct land and/or sea route that was available is used.<p>As an alternative for prehistoric communities, it refers to the distance between largest quasi-capital and furthest village within the quasi-polity.</p>"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range tbs:Integer .

scm:measurement_units
  rdfs:label "Measurement Units"@en ;
  rdfs:comment "Encodes the historical presence or absence of different aspects of a measurement system. Textual evidence of a measurement system: measurement units are named in sources (e.g. pound, aroura). Archaeological evidence includes finding containers of standard volume, etc. ('inferred presence')"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:MeasurementSystem .

scm:monetary_system
  rdfs:label "Monetary System"@en ;
  rdfs:comment "The types of currency or exchange used within the political authority."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:MonetarySystem .

scm:most_costly_building
  rdfs:label "Most Costly Building"@en ;
  rdfs:comment "The most impressive or costly building constructed by the political authority"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:Building .

scm:population
  rdfs:label "Population"@en ;
  rdfs:comment "Estimated population; can change as a result of a political authority adding/losing new territories or by population growth/decline within a region"@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range tbs:Integer .

scm:postal_system
  rdfs:label "Postal System"@en ;
  rdfs:comment "The types of postal services provided by the political authority."@en ;
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:range scm:PostalSystem .
 